<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AddStudentServlet</servlet-name>
        <servlet-class>org.example.javaee.class01.servlet.AddStudentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddStudentServlet</servlet-name>
        <url-pattern>/AddStudentServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <servlet-class>org.example.javaee.class01.servlet.DeleteStudentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <url-pattern>/DeleteStudentServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HomeworkServlet</servlet-name>
        <servlet-class>org.example.javaee.class01.servlet.HomeworkServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HomeworkServlet</servlet-name>
        <url-pattern>/HomeworkServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SubmitHomeworkServlet</servlet-name>
        <servlet-class>org.example.javaee.class01.servlet.SubmitHomeworkServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitHomeworkServlet</servlet-name>
        <url-pattern>/SubmitHomeworkServlet</url-pattern>
    </servlet-mapping>




</web-app>